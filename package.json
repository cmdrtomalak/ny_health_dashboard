{
  "name": "nyc-health-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "live": "bun run build && concurrently \"bun run live:server\" \"bun run live:frontend\"",
    "live:frontend": "bun x vite preview --mode production",
    "live:server": "NODE_ENV=production bun server/index.ts",
    "build": "bun x vite build --mode production",
    "build:server": "bun build server/index.ts --outdir ./dist/server --target bun",
    "start": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop ecosystem.config.cjs",
    "restart": "pm2 restart ecosystem.config.cjs",
    "lint": "eslint .",
    "lint:server": "eslint server",
    "migrate:postgres": "bun scripts/migrate-to-postgres.ts"
  },
  "dependencies": {
    "@types/papaparse": "^5.5.2",
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "framer-motion": "^12.23.26",
    "helmet": "^7.0.0",
    "idb-keyval": "^6.2.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.6.0",
    "reconnecting-websocket": "^4.4.0",
    "sqlite3": "^5.0.2",
    "ws": "^8.14.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "pm2": "^6.0.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.0"
  },
  "overrides": {
    "tar": "7.5.3"
  }
}