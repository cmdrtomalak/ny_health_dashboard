{
  "name": "nyc-health-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "live": "vite build && vite preview --port 3000",
    "start": "pm2 start ecosystem.config.cjs",
    "start:server": "pm2 start ecosystem.config.cjs --only health-dashboard-backend",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:server": "eslint server --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@types/papaparse": "^5.5.2",
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "framer-motion": "^12.23.26",
    "helmet": "^7.0.0",
    "idb-keyval": "^6.2.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.6.0",
    "reconnecting-websocket": "^4.4.0",
    "sqlite3": "^5.1.0",
    "ws": "^8.14.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "pm2": "^6.0.14",
    "tsx": "^4.7.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.0"
  }
}
